# AgentRadar Environment Configuration
# Copy this file to .env and configure with your values

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/agentradar
REDIS_URL=redis://localhost:6379

# API Configuration
API_PORT=4000
NEXT_PUBLIC_API_URL=http://localhost:4000

# Authentication
JWT_SECRET=your-jwt-secret-here
SESSION_SECRET=your-session-secret-here

# Stripe Payment Processing
STRIPE_SECRET_KEY=sk_test_your_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_key_here

# Email Service (SendGrid/Resend)
EMAIL_API_KEY=your_email_api_key_here
EMAIL_FROM=noreply@agentradar.app

# Push Notifications
EXPO_PUSH_TOKEN=your_expo_token_here
FCM_SERVER_KEY=your_fcm_key_here

# MCP Server Configuration
MCP_SERVER_PORT=3001
ENABLE_MOCK_DATA=true

# Scraper Configuration
SCRAPER_USER_AGENT=AgentRadar/1.0
SCRAPER_RATE_LIMIT=1000
COURT_BULLETIN_URL=https://example.com/court-bulletins
DEVELOPMENT_APPS_URL=https://example.com/dev-apps

# White Label Configuration
WHITE_LABEL_DOMAIN=app.agentradar.app
ENABLE_WHITE_LABEL=true

# Claude Swarm Configuration
SWARM_MODE=standard
SWARM_CONFIG=./claude-swarm.yml
CLAUDE_SWARM_DEBUG=false

# Development
NODE_ENV=development
DEBUG=agentradar:*

# Monitoring
SENTRY_DSN=your_sentry_dsn_here
LOG_LEVEL=info

# Feature Flags
ENABLE_MOBILE_APP=true
ENABLE_DESKTOP_APP=true
ENABLE_COURT_SCRAPER=true
ENABLE_ESTATE_SEARCH=true
ENABLE_DEVELOPMENT_MONITOR=true

# AI Services
OPENAI_API_KEY=sk-your_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here

# External APIs
GOOGLE_MAPS_API_KEY=your_google_maps_key_here
ZILLOW_API_KEY=your_zillow_key_here
REALTOR_API_KEY=your_realtor_key_here

# Deployment
VERCEL_TOKEN=your_vercel_token_here
DOCKER_REGISTRY=your_registry_here
KUBERNETES_NAMESPACE=agentradar-prod