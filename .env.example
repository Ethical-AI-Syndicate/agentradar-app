# AgentRadar Environment Configuration
# Copy this file to .env and configure with your values

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/agentradar
REDIS_URL=redis://localhost:6379

# API Configuration
API_PORT=4000
NEXT_PUBLIC_API_URL=http://localhost:4000

# Authentication
JWT_SECRET=your-jwt-secret-here
SESSION_SECRET=your-session-secret-here

# Stripe Payment Processing
STRIPE_SECRET_KEY=sk_test_your_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_key_here

# Email Service (SendGrid/Resend)
EMAIL_API_KEY=your_email_api_key_here
EMAIL_FROM=noreply@agentradar.app

# Push Notifications
EXPO_PUSH_TOKEN=your_expo_token_here
FCM_SERVER_KEY=your_fcm_key_here

# MCP Server Configuration
MCP_SERVER_PORT=3001
ENABLE_MOCK_DATA=true

# Scraper Configuration
SCRAPER_USER_AGENT=AgentRadar/1.0
SCRAPER_RATE_LIMIT=1000
COURT_BULLETIN_URL=https://example.com/court-bulletins
DEVELOPMENT_APPS_URL=https://example.com/dev-apps

# White Label Configuration
WHITE_LABEL_DOMAIN=app.agentradar.app
ENABLE_WHITE_LABEL=true

# Claude Swarm Configuration
SWARM_MODE=standard
SWARM_CONFIG=./claude-swarm.yml
CLAUDE_SWARM_DEBUG=false

# Development
NODE_ENV=development
DEBUG=agentradar:*

# Monitoring
SENTRY_DSN=your_sentry_dsn_here
LOG_LEVEL=info

# Feature Flags
ENABLE_MOBILE_APP=true
ENABLE_DESKTOP_APP=true
ENABLE_COURT_SCRAPER=true
ENABLE_ESTATE_SEARCH=true
ENABLE_DEVELOPMENT_MONITOR=true

# =============================================================================
# AI SERVICES CONFIGURATION
# =============================================================================

# OpenAI Configuration (Primary AI Service)
OPENAI_API_KEY=sk-your_openai_key_here
OPENAI_ORG_ID=your_openai_org_id_here
OPENAI_DAILY_BUDGET=100
OPENAI_RATE_LIMIT_RPM=60
OPENAI_DEFAULT_MODEL=gpt-4o-mini
OPENAI_PREMIUM_MODEL=gpt-4-turbo-preview

# Anthropic Configuration (Backup AI Service)
ANTHROPIC_API_KEY=your_anthropic_key_here
ANTHROPIC_MODEL=claude-3-sonnet

# AI Performance Settings
AI_MAX_RETRIES=3
AI_BASE_RETRY_DELAY=1000
AI_REQUEST_TIMEOUT=30000
AI_RATE_LIMIT_WINDOW=60000

# =============================================================================
# REAL ESTATE DATA PROVIDERS
# =============================================================================

# Google Maps API (Address Validation & Geocoding)
GOOGLE_MAPS_API_KEY=your_google_maps_key_here
GOOGLE_PLACES_API_KEY=your_google_places_key_here

# Property Data APIs
ZILLOW_API_KEY=your_zillow_key_here
REALTOR_API_KEY=your_realtor_key_here
REDFIN_API_KEY=your_redfin_key_here

# Rental Market Data
RENTSPREE_API_KEY=your_rentspree_key_here
RENTALS_COM_API_KEY=your_rentals_com_key_here
PADMAP_API_KEY=your_padmap_key_here

# Market Intelligence
CORELOGIC_API_KEY=your_corelogic_key_here
ATTOM_DATA_API_KEY=your_attom_data_key_here
RENTOMETER_API_KEY=your_rentometer_key_here

# Neighborhood & Demographics
WALK_SCORE_API_KEY=your_walkscore_key_here
CENSUS_API_KEY=your_census_api_key_here
STATS_CANADA_API_KEY=your_stats_canada_key_here

# Property Assessment (Canadian)
MPAC_API_KEY=your_mpac_api_key_here
BCAAA_API_KEY=your_bcaaa_key_here

# Legal & Title Data
FIRST_AMERICAN_API_KEY=your_first_american_key_here
STEWART_TITLE_API_KEY=your_stewart_title_key_here

# Property History & Records
PROPSTREAM_API_KEY=your_propstream_key_here
DATA_TREE_API_KEY=your_data_tree_key_here

# Repliers MLS Integration
REPLIERS_API_KEY=your_repliers_api_key_here
REPLIERS_ENDPOINT=https://api.repliers.ca/v1
REPLIERS_REGION=GTA
REPLIERS_RATE_LIMIT=100

# Traditional MLS Providers (Optional)
TREB_ENABLED=false
TREB_API_KEY=your_treb_key_here
TREB_SECRET=your_treb_secret_here

REALTOR_CA_ENABLED=false
REALTOR_CA_API_KEY=your_realtor_ca_key_here

CRMLS_ENABLED=false
CRMLS_USERNAME=your_crmls_username
CRMLS_PASSWORD=your_crmls_password

# Custom MLS Providers
# Configured via Admin API - no environment variables needed

# Analytics
GOOGLE_ANALYTICS_ID=G-LHSTSYPHZ2
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-LHSTSYPHZ2
MIXPANEL_TOKEN=your_mixpanel_token_here

# Deployment
DOCKER_REGISTRY=your_registry_here
KUBERNETES_NAMESPACE=agentradar-prod
