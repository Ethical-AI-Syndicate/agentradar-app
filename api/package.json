{
  "name": "@agentradar/api",
  "version": "1.0.0",
  "description": "AgentRadar API Server - Real Estate Intelligence Platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "node build.js",
    "dev": "nodemon src/index.ts",
    "start": "node dist/index.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "ts-node src/seed.ts",
    "test": "COLLECT_COVERAGE=false jest",
    "test:watch": "COLLECT_COVERAGE=false jest --watch",
    "test:coverage": "COLLECT_COVERAGE=true jest --coverage",
    "test:ci": "COLLECT_COVERAGE=true jest --ci --coverage --watchAll=false --bail=1",
    "test:unit": "COLLECT_COVERAGE=true jest --testPathPattern=unit --coverage",
    "test:integration": "COLLECT_COVERAGE=true jest --testPathPattern=integration --coverage",
    "test:security": "COLLECT_COVERAGE=true jest --testPathPattern=security --coverage --testTimeout=60000",
    "test:performance": "COLLECT_COVERAGE=false jest --testPathPattern=performance --testTimeout=300000",
    "test:changed": "COLLECT_COVERAGE=false jest --onlyChanged",
    "test:specific": "COLLECT_COVERAGE=false jest --testNamePattern",
    "test:debug": "COLLECT_COVERAGE=false node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:performance:report": "node scripts/generate-performance-report.js",
    "test:performance:regression": "node scripts/check-performance-regression.js",
    "quality:gate": "node scripts/quality-gate.js",
    "quality:report": "node scripts/generate-quality-report.js",
    "lint": "eslint src/ --ext .ts,.js",
    "lint:fix": "eslint src/ --ext .ts,.js --fix",
    "type-check": "tsc --noEmit",
    "pre-commit": "npm run lint && npm run type-check && npm run test:changed",
    "pre-push": "npm run test:ci"
  },
  "keywords": [
    "real-estate",
    "api",
    "property-intelligence",
    "mls"
  ],
  "author": "AgentRadar",
  "license": "MIT",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.19.13",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.15.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "@sendgrid/mail": "^8.1.5",
    "@socket.io/redis-adapter": "^8.3.0",
    "@types/ioredis": "^4.28.10",
    "axios": "^1.7.9",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^5.1.1",
    "morgan": "^1.10.1",
    "openai": "^5.20.0",
    "socket.io": "^4.8.1",
    "stripe": "^18.5.0"
  }
}
