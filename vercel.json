{
  "version": 2,
  "installCommand": "npm install && cd web-app && npm install",
  "buildCommand": "cd web-app && npm run build",
  "outputDirectory": "web-app/.next",
  "framework": "nextjs",
  "ignoreCommand": "false",
  "env": {
    "NODE_ENV": "production",
    "SKIP_HUSKY_INSTALL": "1",
    "DATABASE_URL": "@database_url",
    "JWT_SECRET": "@jwt_secret",
    "SESSION_SECRET": "@session_secret",
    "REDIS_URL": "@redis_url"
  }
}